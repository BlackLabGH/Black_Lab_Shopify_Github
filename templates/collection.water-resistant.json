og.<\/p>","size":100}},"button_YXL67L":{"type":"button","disabled":true,"settings":{"button_text":"Read More","button_link":"","button_color":"btn--black","button_style":"btn--solid","button_size":"btn--small"}}},"block_order":["subheading_it9Y8X","heading_E4eQeg","text_WK8Dqm","button_YXL67L"],"disabled":true,"settings":{"show_spacer_lines":true,"text_position":"left","bg_color_secondary":false,"text_color":"text-dark","width":"wrapper","padding_top":30,"padding_bottom":0}},"main":{"type":"collection","blocks":{"25fdead2-cb7b-47cc-a6f1-58909d5b04ff":{"type":"promo","disabled":true,"source":"{{ collection.metafields.custom.collection_inline_blog.value }}","settings":{"collection":"","image":"{{ collection.metafields.custom.collection_inline_blog.value.inline_collection_blog_image.value }}","position":1,"width":"auto","title":"{{ collection.metafields.custom.collection_inline_blog.value.inline_collection_blog_body | metafield_tag }}","heading_highlight_type":"circle","highlight_color":"#d43747","highlight_text_color":"#ffffff","heading_size":100,"content":"Advertise a promotion or a sale","text_size":100,"text_color":"text-light","text_position":"bottom-left","layout":"stacked","overlay_opacity":10,"bg_color":"#d3d3d3","button_text":"{{ collection.metafields.custom.collection_inline_blog.value.inline_collection_blog_header.value }}","button_link":"","button_color":"btn--white","button_style":"btn--solid-border","button_size":"btn--small"}}},"block_order":["25fdead2-cb7b-47cc-a6f1-58909d5b04ff"],"settings":{"show_breadcrumbs":true,"enable_sorting":true,"enable_filters":false,"filters_layout":"open","show_more":true,"filter_mode":"default","heading_style":"body","bg_color":"","padding_top":30,"padding_bottom":10}},"section_collection_bXNimr":{"type":"section-collection","name":"Featured collection","settings":{"collection":"dog-beds","products_layout":"grid","number_of_products":8,"products_per_row_on_desktop":4,"columns_mobile":"1","text_alignment":"text-left","title"